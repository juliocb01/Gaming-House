<h1>Editing Bootcamp</h1>

<%= simple_form_for(@bootcamp) do |f|  %>
  <%= f.input :name  %>
  <%= f.input :address %>
  <%= f.input :price %>
  <%= f.input :pc_number %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: 'btn btn-success btn-rounded'%>
  <%# <%= raise %>
<% end %>

<%= link_to 'Back', bootcamps_path %>
