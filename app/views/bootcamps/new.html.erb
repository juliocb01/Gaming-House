<div class="container">
  <h1>Create a bootcamp</h1>

<%= simple_form_for @bootcamp do |f|  %>
  <%= f.input :name  %>
  <%= f.input :address %>
  <%= f.input :price %>
  <%= f.input :pc_number %>
  <%= f.submit class: 'btn btn-success btn-rounded'%>
  <%# <%= raise %>
<% end %>

<%= link_to 'Back', bootcamps_path %>

</div>
